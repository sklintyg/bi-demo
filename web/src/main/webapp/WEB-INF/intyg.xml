<Schema name='Intyg' metamodelVersion='4.0'>
  <PhysicalSchema>
    <Table name='fact_intyg'>
      <Key>
        <Column name='ID'/>
      </Key>
    </Table>

    <Table name='dim_intygstyp'>
      <Key>
        <Column name='typ'/>
      </Key>
    </Table>

    <Table name='dim_icd'>
      <Key>
        <Column name='ICD_CODE'/>
      </Key>
    </Table>

    <Table name='dim_vardgivare'>
      <Key>
        <Column name='HSA_ID'/>
      </Key>
    </Table>


    <Table name='dim_vardenhet'>
      <Key>
        <Column name='HSA_ID'/>
      </Key>
    </Table>


    <Link target='dim_vardenhet' source='dim_vardgivare'>
      <ForeignKey>
        <Column name='VARDGIVARE_ID'/>
      </ForeignKey>
    </Link>

  </PhysicalSchema>

  <Cube name='Intyg' defaultMeasure='Antal intyg'>
    <Dimensions>

      <Dimension name='Intygstyp' table='dim_intygstyp' key="Typ">
        <Attributes>
          <Attribute name='Typ' keyColumn='typ' hasHierarchy='true'/>
        </Attributes>
      </Dimension>
      <Dimension name='Diagnoskod' table='dim_icd' key="ICD_CODE">
        <Attributes>
          <Attribute name='ICD_CODE' keyColumn='ICD_CODE' hasHierarchy='true'/>
        </Attributes>
      </Dimension>


      <Dimension name='Vardenhet' table='dim_vardenhet' key="VardenhetId">
        <Attributes>

          <Attribute name="VardgivareId" keyColumn="HSA_ID" table="dim_vardgivare" hasHierarchy='true' visible="false"></Attribute>

          <Attribute name='VardenhetId' table='dim_vardenhet' keyColumn='HSA_ID' hasHierarchy='false'/>

          <Attribute name='Vardenhetsnamn' table='dim_vardenhet' hasHierarchy='false' orderByColumn="VARDENHET_NAMN"
              keyColumn='HSA_ID' nameColumn='VARDENHET_NAMN'/>

        </Attributes>

        <Hierarchies>
          <Hierarchy name='Vardenheter'>
            <Level attribute='VardgivareId'/>
            <Level attribute='Vardenhetsnamn'/>
            <Level attribute='VardenhetId'/>
          </Hierarchy>
        </Hierarchies>
      </Dimension>

      <Dimension name='Ar' table='fact_intyg' key="Ar">
        <Attributes>
          <Attribute name='Ar' keyColumn='SIGN_YEAR' hasHierarchy='true'/>
        </Attributes>
      </Dimension>

      <Dimension name="Kon" table="fact_intyg" key="Kon">
        <Attributes>
          <Attribute name='Kon' keyColumn='GENDER' hasHierarchy='true'/>
        </Attributes>
      </Dimension>

